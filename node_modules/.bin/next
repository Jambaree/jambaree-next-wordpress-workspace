#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../@jambaree/next-wordpress/node_modules/next/dist/bin/next" "$@"
  ret=$?
else 
  node  "$basedir/../../@jambaree/next-wordpress/node_modules/next/dist/bin/next" "$@"
  ret=$?
fi
exit $ret
